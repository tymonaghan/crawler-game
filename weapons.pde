class Weapon{
  int baseDamage, damageType, weaponCategory;
  String weaponName, weaponNote;

  Weapon(int dmg_, int dmgtype_, int category_, String name_, String note_){
    baseDamage = dmg_;
    damageType = dmgtype_;
    weaponCategory = category_;
    weaponName = name_;
    weaponNote = note_;
  } //end weapon constructor
} //end Weapons class
